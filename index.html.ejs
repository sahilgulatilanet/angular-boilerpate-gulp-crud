<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>angular-boilerplate</title>
  <!--<link rel="stylesheet" href="<%= asset('app.css') %>">-->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.0/umd/popper.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js"></script>
</head>
<body ng-cloak ng-app='app'>
  <div ng-include="'/templates/nav.html'"></div>
  <div ng-view></div>
  <% if (production) { %>
  <script src="<%= asset('angular/angular.min.js') %>"></script>
  <script src="<%= asset('angular-route/angular-route.min.js') %>"></script>
  <% } else { %>
  <script src="<%= asset('angular/angular.js') %>"></script>
  <script src="<%= asset('angular-route/angular-route.js') %>"></script>
  <% } %>
  <script src="<%= asset('app.js') %>"></script>
</body>
</html>
